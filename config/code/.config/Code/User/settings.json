{
    "git.autofetch": true,
    "workbench.colorTheme": "Default Light+",
    "workbench.preferredHighContrastColorTheme": "Default High Contrast Light",
    "editor.fontFamily": "'Cascadia Mono', 'CaskaydiaCove NFM', 'Droid Sans Mono', 'monospace', monospace",
    "workbench.colorCustomizations": {
        
        "activityBar.background": "#f35a5a",
        "activityBar.activeBackground": "#ffa6a6",
        "statusBar.background" : "#ffa6a6"
    },
    "editor.fontWeight": "normal",
    "git.enableSmartCommit": true,
    "typescript.preferences.quoteStyle": "single",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "workbench.editorAssociations": {
        "*.zip": "default"
    },
    "tabnine.experimentalAutoImports": true,
    "[python]": {
        "editor.formatOnType": true
    },
    "editor.fontSize": 13,
    "editor.inlineSuggest.enabled": true,
    "workbench.iconTheme": "material-icon-theme",
    "window.zoomLevel": 1,
    "nxConsole.showNodeVersionOnStartup": false
}